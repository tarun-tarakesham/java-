public class tara
{
	public static void main(String args[])
	{
		System.out.print("hello tara");
		System.out.print("hi tarun 1");
		System.out.print("hi tarun 2");
		System.out.print("hi tarun 3");
		System.out.print("hi tarun 4");
		System.out.print("hi tarun 5");
	}
}

output:
hello tarahi tarun 1hi tarun 2hi tarun 3hi tarun 4hi tarun 5


